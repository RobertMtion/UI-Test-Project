.timeline-main-bar {
	background-color: rgb(64, 64, 64);
	height: auto;
	width: 900px;
	flex-direction: column;
	flex-wrap: wrap;
	align-content: flex-start;
	align-items: flex-start;
	justify-content: flex-start;
}

.timeline-indicator-container {
	width: 80%;
	display: flex;
	flex-direction: row;
	padding-right: 10px;
	padding-left: 10px;
	margin-left: auto;
}

.timeline-indicator {
	width: 1px;
	height: 7px;
	background-color: rgb(0, 0, 0);
}

.timeline-indicator.medium {
	height: 12px !important;
}

.timeline-indicator.large {
	height: 17px !important;
	width: 2px !important;
}

.timeline-indicator-padding {
	flex-grow: 1;
}

.timeline-track {
	height: 30px;
	width: 100%;
	margin-top: 5px;
	margin-bottom: 5px;
	background-color: rgb(96, 96, 96);
	flex-direction: row;
	flex-wrap: wrap;
	align-content: flex-start;
	align-items: flex-start;
	justify-content: flex-start;
}

.timeline-track.selected {
	background-color: rgb(200, 200, 200);
}

.timeline-track-header-container {
	width: 20%;
	height: 100%;
	border-right-width: 1px;
	border-right-color: rgb(0, 0, 0);
}

.timeline-track-keyframe-container {
	width: 80%;
	height: 100%;
}

.keyframe-element {
	background-color: rgb(164, 164, 164);
	width: 20px;
	height: 20px;
	margin-top: 2.5px;
	border-radius: 5px;
	overflow: visible;
	position: absolute;
}

.keyframe-element.selected {
	border-width: 2px;
	border-color: rgb(255, 0, 0);
}